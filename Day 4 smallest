#include <stdio.h>

int main(void) 
{
  int i,n,s;
  float a[100];
  printf("Enter number of integers=");
  scanf("%d",&n);
  for(i=0;i<n;++i)
  {
    printf("\nEnter value=");
    scanf("%f",&a[i]);
  }

  s=a[0];
  i=1;
  while(i<n){
    if(s>a[i])
    {
      s=a[i];
    }
    else
    {
      i++;
    }
  }
  printf("Smallest number=");
  printf("%d",s);
  return 0;
}
