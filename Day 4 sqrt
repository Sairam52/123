#include <stdio.h>
#include <math.h>

int main(void)
{
  int n,i,p;
  float s=0,x=0;
  printf("Enter the number of entries : ");
  scanf("%d",&n);
  for(p=1;p<=n;p++)
  {
    printf("Enter the numbers : \n");
    scanf("%d",&i);
    x=sqrt(i);
    s=s+x;
    
  }
  printf("The answer is : %f",s);
  return 0;
}
